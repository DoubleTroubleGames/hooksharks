code = "shader_type canvas_item;

uniform float frequency = 60;
uniform float depth = 0.005;
uniform float amount : hint_range(0,5);

void fragment() {
	
	vec2 uv = SCREEN_UV;
	uv.y += sin(uv.x*frequency+TIME)*depth;
	uv.y = clamp(uv.y,0,1);
	vec3 c = textureLod(SCREEN_TEXTURE,uv,amount).rgb;
	
	COLOR.rgb = c;
}
"

